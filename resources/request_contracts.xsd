<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:complexType name="root">
        <xs:sequence>
            <xs:element name="ContractId" description="Унікальний системний ідентифікатор договору про закупівлю" type="xs:string"/>
            <xs:element name="Documents">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element maxOccurs="unbounded" ref="File"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="File" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>